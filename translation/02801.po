msgstr ""

#: 02801:454

msgid "{Daigo}\n\"What did you do that for!?\""
msgstr ""

#: 02801:457

msgid "Bang!!"
msgstr ""

#: 02801:458

msgid "{Appo}\n\"Ugggah!\""
msgstr ""

#: 02801:462

